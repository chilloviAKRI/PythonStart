
def setup():
    global x1,y1
    global x1Change,y1Change
    background(3,31,139)
    
    size(800,800)
    
    x1 = random(0,800)
    y1 = random(0,800)
    x1Change = random(0,10)
    y1Change = random(0,10)
    
def draw():
    global x1,x2,y1
    global x1Change,y1Change
    stroke(random(0,255),random(0,255),random(0,255))
    strokeWeight(random(10,16))
    
    #background(100)
    point(x1,y1)
    
    x1 = random(0,800)
    y1 = random(0,800)
